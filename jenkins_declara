pipeline{
    agent any
    
    environmnet{
        PATH='/opt/maven/bin:$PATH'
    }
    stages{
        stage("Git checkout"){
            steps{
                git 'https://github.com/javahometech/myweb.git'
            }
        }
        stage("Maven Build"){
            steps{
               sh "mvn clean package"
            } 
        }
    }
}
